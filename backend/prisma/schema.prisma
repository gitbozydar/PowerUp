generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           Int      @id @default(autoincrement())
  name         String?
  email        String   @unique
  passwordHash String?
  provider     String?
  providerId   String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime
}
